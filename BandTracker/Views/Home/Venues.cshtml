<h1>Band Tracker</h1>
<hr />
<h2>Venues:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var venue in Model)
    {
      <li><a href="/venues/@venue.GetId()">@venue.GetVenueName()</a>
      <h4><a href="/venues/@venue.GetId()/edit">Edit</a></h4></li>
      <h4><a href="/@venue.GetVenueName()/@venue.GetId()/venue/delete">Delete</a></h4>
    }
  </ul>
}
<h4><a href="/venues/new">Add a new venue</a></h4>
<h4><a href="/">Return home</a></h4>
